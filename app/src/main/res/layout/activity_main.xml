        android:textColor="#FEFEFF"
        android:textSize="16sp"
        app:backgroundTint="#FEFEFF"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/result2" />

    <TextView
