<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/space"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/result2"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        android:textColor="#FEFEFF"

        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/result2"
        app:layout_constraintTop_toBottomOf="@+id/other"
        tools:ignore="NotSibling" />

    <EditText
        android:id="@+id/result3"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        android:textColor="#FEFEFF"
        android:textSize="16sp"
        app:backgroundTint="#FEFEFF"
        app:layout_constraintStart_toEndOf="@+id/result3"
        app:layout_constraintTop_toBottomOf="@+id/posthone"
        tools:ignore="NotSibling" />

    <EditText
        android:id="@+id/posthone"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:ems="10"
